#!/usr/local/bin/interpreter
program "test elseif";

begin
{
        $a = 1;
        if $a == 1
                print "yes", endl;
        else
                print "no", endl;
        while $a < 10 {
                if $a == 1 {
                        print "one", endl;
                } elseif $a == 2 {
                        print "two", endl;
                } elseif $a == 3 {
                        print "three", endl;
                } elseif $a == 4 {
                        print "four", endl;
                } elseif $a == 5 {
                        print "five", endl;
                } elseif $a == 6 {
                        print "six", endl;
                } elseif $a == 7 {
                        print "seven", endl;
                } elseif $a == 8 {
                        print "eight", endl;
                } else {
                        print "else", endl;
                }
                $a = $a + 1;
        }
}
end

